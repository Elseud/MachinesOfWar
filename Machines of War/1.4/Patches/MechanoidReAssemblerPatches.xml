<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<!--[KIR]Extra Mechanoid Units and Equipment-->
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Factions Expanded - Mechanoids - Mechanoid Re-Assembler</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Normal</success>
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/ThingDef[defName = "REMA_MechAssembler"]/comps/li[3]/spawnablePawnKinds</xpath>
					<value>
						<li>MOW_Mech_Enforcer</li>
						<li>MOW_Mech_Cicada</li>
						<li>MOW_Mech_Demolisher</li>
						<li>MOW_Mech_Bombardier</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>